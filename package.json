{
  "name": "vscode-insert-line-number-and-copy",
  "displayName": "Insert Line Number And Copy",
  "description": "Insert line numbers in text and copy.",
  "version": "0.1.0",
  "publisher": "SatoshiYamamoto",
  "author": {
    "name": "Satoshi Yamamoto",
    "email": "standard.software.net@gmail.com",
    "url": "https://www.facebook.com/stndardsoftware"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/standard-software/vscode-insert-line-number-and-copy"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "line number",
    "template"
  ],
  "activationEvents": [
    "onCommand:InsertLineNumberAndCopy.InsertStartZero",
    "onCommand:InsertLineNumberAndCopy.CopyFileLineNumer"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "InsertLineNumberAndCopy.InsertStartZero",
        "title": "Insert Line Number And Copy | Insert | start 0"
      },
      {
        "command": "InsertLineNumberAndCopy.CopyFileLineNumer",
        "title": "Insert Line Number And Copy | Copy | start File Line Number "
      }
    ],
    "configuration": {}
  },
  "scripts": {
    "lint": "eslint .",
    "": ""
  },
  "devDependencies": {
    "eslint": "^7.32.0"
  },
  "dependencies": {
    "clipboardy": "^2.3.0"
  }
}
